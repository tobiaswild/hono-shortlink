{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "verbatimModuleSyntax": true,
    "skipLibCheck": true,
    "types": ["@types/node", "node"],
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "outDir": "./dist",
    "moduleResolution": "nodenext",
    "baseUrl": "."
  },
  "exclude": ["node_modules"],
  "include": ["server/src/**/*"]
}
